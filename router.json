{
  "inbounds": [
    {
      "type": "tun",
      "interface_name": "tun0",
      "stack": "system",
      "address": "172.19.0.1/30",
      "exclude_uid": [
        1000
      ],
      "auto_route": true,
      "strict_route": false
    },
    {
      "type": "http",
      "listen": "127.0.0.1",
      "listen_port": 7890
    }
  ],
  "route": {
    "rules": [
      {
        "inbound": "in",
        "action": "sniff"
      }
    ]
  },
  "experimental": {
    "clash_api": {
      "external_controller": "0.0.0.0:9090",
      "external_ui_download_url": "https://github.com/Zephyruso/zashboard/releases/latest/download/dist.zip",
      "external_ui_download_detour": "direct",
      "external_ui": "ui",
      "default_mode": "Rule"
    }
  }
}
