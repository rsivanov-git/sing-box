{
  "log": {
    "disabled": false,
    "level": "error",
    "timestamp": true
  },
  "dns": {
    "servers": [
      {
        "tag": "proxy",
        "address": "https://1.1.1.1/dns-query"
      },
      {
        "tag": "direct",
        "address": "https://1.1.1.1/dns-query",
        "detour": "direct"
      },
      {
        "tag": "russian",
        "address": "https://1.1.1.1/dns-query",
        "detour": "direct"
      },
      {
        "tag": "block",
        "address": "rcode://refused"
      }
    ],
    "rules": [
      {
        "outbound": "any",
        "server": "direct"
      },
      {
        "clash_mode": "Direct",
        "server": "direct"
      },
      {
        "clash_mode": "Global",
        "server": "proxy"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "rule_set": "geosite-yandex"
          },
          {
            "rule_set": "geosite-vk"
          },
          {
            "rule_set": "direct-russian"
          }
        ],
        "server": "russian"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "rule_set": "geosite-apple"
          },
          {
            "rule_set": "geosite-telegram"
          },
          {
            "rule_set": "geosite-twitch"
          },
          {
            "rule_set": "geosite-whatsapp"
          },
          {
            "rule_set": "geosite-zoom"
          },
          {
            "rule_set": "direct-foreign"
          }
        ],
        "server": "direct"
      }
    ],
    "final": "proxy",
    "strategy": "ipv4_only",
    "reverse_mapping": true,
    "disable_cache": false,
    "disable_expire": false,
    "independent_cache": true
  },
  "inbounds": [
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct"
    }
  ],
  "route": {
    "rule_set": [
      {
        "type": "remote",
        "tag": "direct-russian",
        "format": "source",
        "url": "https://raw.githubusercontent.com/rsivanov-git/sing-box/main/direct-russian.json"
      },
      {
        "type": "remote",
        "tag": "direct-foreign",
        "format": "source",
        "url": "https://raw.githubusercontent.com/rsivanov-git/sing-box/main/direct-foreign.json"
      },
      {
        "type": "remote",
        "tag": "proxied",
        "format": "source",
        "url": "https://raw.githubusercontent.com/rsivanov-git/sing-box/main/proxied.json"
      },
      {
        "type": "remote",
        "tag": "geosite-yandex",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-yandex.srs"
      },
      {
        "type": "remote",
        "tag": "geosite-vk",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-vk.srs"
      },
      {
        "type": "remote",
        "tag": "geosite-apple",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-apple.srs"
      },
      {
        "type": "remote",
        "tag": "geosite-telegram",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-telegram.srs"
      },
      {
        "type": "remote",
        "tag": "geosite-twitch",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-twitch.srs"
      },
      {
        "type": "remote",
        "tag": "geosite-whatsapp",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-whatsapp.srs"
      },
      {
        "type": "remote",
        "tag": "geosite-zoom",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-zoom.srs"
      },
      {
        "type": "remote",
        "tag": "geoip-ru",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geoip/rule-set/geoip-ru.srs"
      }
    ],
    "rules": [
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "protocol": "dns"
          },
          {
            "port": 53
          }
        ],
        "action": "hijack-dns"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "port": 853
          },
          {
            "network": "udp",
            "port": 443
          }
        ],
        "action": "reject"
      },
      {
        "ip_is_private": true,
        "outbound": "direct"
      },
      {
        "clash_mode": "Direct",
        "outbound": "direct"
      },
      {
        "clash_mode": "Global",
        "outbound": "proxy"
      },
      {
        "rule_set": "proxied",
        "outbound": "proxy"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "rule_set": "geosite-yandex"
          },
          {
            "rule_set": "geosite-vk"
          },
          {
            "rule_set": "geosite-apple"
          },
          {
            "rule_set": "geosite-telegram"
          },
          {
            "rule_set": "geosite-twitch"
          },
          {
            "rule_set": "geosite-whatsapp"
          },
          {
            "rule_set": "geosite-zoom"
          },
          {
            "rule_set": "geoip-ru"
          },
          {
             "rule_set": "direct-russian"
          },
          {
            "rule_set": "direct-foreign"
          }
        ],
        "outbound": "direct"
      },
      {
        "protocol": "quic",
        "action": "reject"
      }
    ],
    "final": "proxy",
    "auto_detect_interface": true
  },
  "experimental": {
    "cache_file": {
      "enabled": true
    }
  }
}
